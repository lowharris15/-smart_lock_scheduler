# Flask configuration
SECRET_KEY=dev-secret-key
PORT=5000
DEBUG=True

# Time configuration
# Timezone for displaying times (e.g., "America/New_York", "Europe/London", "Asia/Tokyo")
# See https://en.wikipedia.org/wiki/List_of_tz_database_time_zones for valid options
TIMEZONE=UTC

# Seam API configuration
# Replace with your actual Seam API key
SEAM_API_KEY=your_seam_api_key_here

# Email configuration
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
FROM_EMAIL=your_email@gmail.com 